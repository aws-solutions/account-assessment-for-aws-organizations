{
  "name": "account-assessment-for-aws-organizations-webui",
  "version": "1.0.14",
  "description": "Account Assessment for AWS Organizations (SO0217)",
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com/solutions",
    "organization": true
  },
  "private": true,
  "dependencies": {
    "@aws-amplify/api": "^5.4.14",
    "@aws-amplify/auth": "^5.6.13",
    "@aws-amplify/core": "^5.8.13",
    "@cloudscape-design/collection-hooks": "^1.0.54",
    "@cloudscape-design/components": "^3.0.787",
    "@cloudscape-design/design-tokens": "^3.0.44",
    "@cloudscape-design/global-styles": "^1.0.32",
    "@types/node": "^20.16.11",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^3.5.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "test:ci": "CI=true react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.13",
    "babel-jest": "^29.7.0",
    "msw": "^1.3.4",
    "renamer": "^4.0.0"
  },
  "overrides": {
    "svgo": {
      "css-select": "~4.3.0"
    },
    "resolve-url-loader": {
      "postcss": "8.4.31"
    },
    "msw": {
      "path-to-regexp": ">=8.1.0"
    },
    "path-to-regexp": ">=8.1.0"
  },
  "jest": {
    "testMatch": [
      "**/*.test.ts(x)?"
    ],
    "transform": {
      "^.+\\.tsx?$": "babel-jest",
      ".+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@cloudscape-design/components)/"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts*",
      "!<rootDir>/node_modules/",
      "!/build/"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "<rootDir>/src/Empty.ts"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
