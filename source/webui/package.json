{
  "name": "account-assessment-for-aws-organizations-webui",
  "version": "1.0.9",
  "description": "Account Assessment for AWS Organizations (SO0217)",
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com/solutions",
    "organization": true
  },
  "private": true,
  "dependencies": {
    "@aws-amplify/api": "^5.4.12",
    "@aws-amplify/auth": "^5.6.12",
    "@aws-amplify/core": "^5.8.12",
    "@cloudscape-design/collection-hooks": "^1.0.9",
    "@cloudscape-design/components": "^3.0.72",
    "@cloudscape-design/design-tokens": "^3.0.3",
    "@cloudscape-design/global-styles": "^1.0.1",
    "@types/node": "^20.4.9",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "bootstrap": "^5.2.1",
    "date-fns": "^2.29.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^3.0.2",
    "webpack-dev-middleware": ">=5.3.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "test:ci": "CI=true react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.0",
    "babel-jest": "^29.1.0",
    "msw": "^1.3.2",
    "renamer": "^4.0.0"
  },
  "overrides": {
    "@aws-sdk/client-cloudwatch-logs": {
      "@aws-sdk/middleware-retry": "~3.110.0"
    },
    "@aws-sdk/client-cognito-identity": {
      "@aws-sdk/middleware-retry": "~3.110.0"
    },
    "svgo": {
      "css-select": "~4.3.0"
    },
    "resolve-url-loader": {
      "postcss": "8.4.31"
    }
  },
  "jest": {
    "testMatch": [
      "**/*.test.ts(x)?"
    ],
    "transform": {
      "^.+\\.tsx?$": "babel-jest",
      ".+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@cloudscape-design/components)/"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts*",
      "!<rootDir>/node_modules/",
      "!/build/"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "<rootDir>/src/Empty.ts"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
